{
  "action": {
    "default_popup": "index.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "js": [
        "./content.js"
      ],
      "matches": [
        "*://google.com/*"
      ],
      "run_at": "document_end"
    }
  ],
  "description": "The description of your extension",
  "icons": {
    "16": "icons/icon.png",
    "32": "icons/icon.png",
    "48": "icons/icon.png",
    "128": "icons/icon.png"
  },
  "manifest_version": 3,
  "name": "Chrome Extension Starter V3",
  "version": "1.0"
}